#!/usr/bin/env node

var program = require('commander');
var GenerateCommand = require('../lib/index.js').GenerateCommand;

program
  .command('generate')
  .arguments('<name>')
  .description('Generate a new action directory and files')
  .action(function (name, command) {
    new GenerateCommand({
      command: command,
      name: name
    }).run()
  });

program.parse(process.argv);
